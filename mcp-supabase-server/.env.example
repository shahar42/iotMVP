# MCP Supabase Server Configuration
# Copy this file to .env and update with your actual values

# Supabase Database URL (same as your main project)
DATABASE_URL=postgresql://postgres:your-password@your-project.pooler.supabase.com:5432/postgres

# Optional: Individual database components (if DATABASE_URL not set)
DB_USER=postgres.your-project-ref
DB_PASS=your-password
DB_HOST=aws-0-us-east-1.pooler.supabase.com
DB_PORT=5432
DB_NAME=postgres

# Logging level
LOG_LEVEL=INFO